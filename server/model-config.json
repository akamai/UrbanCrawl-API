{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db_name",
    "public": false,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "AccessToken": {
    "dataSource": "db_name",
    "public": false
  },
  "ACL": {
    "dataSource": "db_name",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db_name",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db_name",
    "public": false
  },
  "city": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllCities": true,
          "getCityDetails": true,
          "search": true,
          "getPlaceDetails": true,
          "getPlacesOfCity": true,
          "getMediaOfPlace": true,
          "getMediaOfCity": true
        }
      }
    }
  },
  "place": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllPlacesOfCity": false,
          "getPlaceDetails": false
        }
      }
    }
  },
  "media": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllMediaByPlaceId": false,
          "getAllMediaByCityId": false
        }
      }
    }
  },
  "cart": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "addToCart": true,
          "updateCart": false,
          "getCart": true,
          "checkout":true
        }
      }
    }
  },
  "order": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "checkout": false,
          "getOrders": false
        }
      }
    }
  },
  "customer": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "register": true,
          "login": true
        }
      }
    }
  },
  "keypairs": {
  "login": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getPublicKey": false,
          "regenerateKeys": false
        }
      }
    }
  },
  "token": {
    "dataSource": "db_name",
    "public": false
  }
}
