{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "AccessToken": {
    "dataSource": "db_name",
    "public": false
  },
  "ACL": {
    "dataSource": "db_name",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db_name",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db_name",
    "public": false
  },
  "city": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllCities": true,
          "getCityDetails": true,
          "search": true
        }
      }
    }
  },
  "place": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllPlacesOfCity": true,
          "getPlaceDetails": true
        }
      }
    }
  },
  "media": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllMediaByPlaceId": true,
          "getAllMediaByCityId": true
        }
      }
    }
  },
  "cart": {
    "dataSource": "db_name",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "addToCart": true,
          "updateCart": true,
          "getCart": true
        }
      }
    }
  },
  "order": {
    "dataSource": "terraplanet-db-heroku",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "checkout": true,
          "getOrders": true
        }
      }
    }
  }
}
